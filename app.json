{
  "expo": {
    "name": "Catwise", 
    "slug": "catwise", 
    "version": "1.0.0",
    "orientation": "portrait", 
    "icon": "./assets/images/icono-logo.png",
    "scheme": "catwise",
    "userInterfaceStyle": "automatic",
    "splash": { 
      "image": "./assets/images/splash.png", 
      "resizeMode": "contain", 
      "backgroundColor": "black" 
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bryancito.catwise"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icono-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true 
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7c3e8f83-b3c5-409e-8ec6-76b232ab12b6"
      }
    }
  
  }
}
